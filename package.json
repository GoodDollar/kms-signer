{
  "name": "kms-ethereum-signing",
  "version": "1.0.0",
  "description": "Native Ethereum transaction signing using AWS KMS with secp256k1 support",
  "packageManager": "yarn@3.6.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "setup": "ts-node src/cli/setup.ts",
    "import": "ts-node src/cli/import.ts",
    "sign": "ts-node src/cli/sign.ts",
    "submit": "ts-node src/cli/submit.ts",
    "list": "ts-node src/cli/list.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "aws",
    "kms",
    "ethereum",
    "blockchain",
    "secp256k1",
    "signing",
    "cryptocurrency"
  ],
  "author": "GoodDollar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GoodDollar/kms-signer.git"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@aws-sdk/client-kms": "^3.490.0",
    "@ethereumjs/common": "^3.1.0",
    "@ethereumjs/tx": "^4.1.1",
    "dotenv": "^16.3.1",
    "web3": "^1.10.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
